<?xml version="1.0" encoding="utf-8" ?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <% urls.forEach(function(url) { %>
    <url>
      <loc><%= url.loc %></loc>
      <% if(url.priority) { %>
        <priority><%= url.priority %></priority>
      <% } %>
    </url>
  <% }); %>
</urlset>
