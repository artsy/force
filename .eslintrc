{
  "parser": "babel-eslint",
  "extends": [
    "prettier",
    "standard",
    "standard-react"
  ],
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "camelcase": 0,
    "comma-dangle": ["error", "always-multiline"],
    "no-mixed-operators": 0,
    "no-new": 0,
    "react/jsx-indent": 0
  }
}
