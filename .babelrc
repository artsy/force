{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }],
    "react",
    "stage-3"
  ],
  "plugins": [
    "babel-plugin-rewire",
    "inline-react-svg",
    "transform-runtime",
    ["module-resolver", {
      "root": ["./"]
    }],
    "transform-class-properties"
  ],
  "env": {
    "development": {
      "plugins": [
        ["styled-components", {
          "ssr": true
        }]
      ]
    }
  }
}
