  "jest": {
    "transform": {
      "^.+\\.coffee$": "<rootDir>/node_modules/jest-coffee-preprocessor/index.js",
      ".(ts|tsx|js|jsx)": "babel-jest"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleFileExtensions": [
      "coffee",
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "testRegex": ".*\\.jest\\.(ts|tsx|js|jsx)$",
    "setupFiles": [
      "<rootDir>/test.config.js"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testURL": "https://artsy.net"
  },
  "reporterEnabled": "mocha-junit-reporter, spec",
  "mochaJunitReporterReporterOptions": {
    "mochaFile": "reports/mocha/test-results.xml"
  },
  "nyc": {
    "check-coverage": true,
    "temp-dir": "./.nyc_output/",
    "report-dir": "./coverage/",
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".coffee",
      ".js"
    ],
    "lines": 0
