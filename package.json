  "private": true,
  "engines": {
    "node": "10",
    "yarn": "1.x.x"
  },
  "scripts": {
    "acceptance": "scripts/acceptance.sh",
    "acceptance-record": "scripts/acceptance_record.sh",
    "assets": "scripts/assets.sh",
    "build": "yarn webpack",
    "bundle-report": "NODE_ENV=production ANALYZE_BUNDLE=true yarn build",
    "bundle-report:dev": "NODE_ENV=development ANALYZE_BUNDLE=true yarn build",
    "clean": "rm -rf public/assets",
    "mocha": "scripts/mocha.sh",
    "precommit": "lint-staged",
    "prepare": "patch-package",
    "prepush": "yarn type-check",
    "prettier-project": "yarn run prettier-write 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier-write": "yarn run prettier --write",
    "publish_assets": "scripts/publish_assets.sh",
    "relay": "node scripts/relayCompiler.js",
    "sentry": "scripts/sentry.sh",
    "start": "scripts/start.sh",
    "start:prod": "NODE_ENV=production node -r dotenv/config --max_old_space_size=1024 ./src --verbose --debugProd",
    "start:relay": "yarn relay && concurrently --raw --kill-others 'yarn relay --watch' 'yarn start'",
    "sync-schema": "rm -rf data && mkdir data && graphql-fetch-schema https://metaphysics-staging.artsy.net -o data",
    "test": "scripts/test.sh",
    "type-check": "tsc --pretty --noEmit",
    "webpack": "node --max_old_space_size=4096 -r dotenv/config -r @babel/register node_modules/.bin/webpack --config ./webpack"
  },
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.@(js|jsx|ts|tsx)": [
      "npm run prettier-write --",
      "git add"
    ]
  },
