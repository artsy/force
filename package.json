  "husky": {
    "hooks": {
      "post-merge": "sh scripts/pull-lock.sh",
      "pre-commit": "lint-staged --no-stash",
      "pre-push": "yarn type-check"
    }
  },
  "lint-staged": {
    "*": "yarn detect-secrets:hook",
    "*.@(md)": [
      "yarn format --parser markdown"
    ],
    "*.@(js|jsx|ts|tsx)": [
      "yarn format"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "report-dir": "./.nyc_output/",
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".js"
    ],
    "lines": 0
  },
  "pull-lock": {
    "yarn.lock": "yarn install"
  }
