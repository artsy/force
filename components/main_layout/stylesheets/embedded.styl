//
// List any data-uri embedded assets here so they are only embedded once.
//

@font-face
  font-family 'artsy-icons'
  src url('/fonts/artsy-icons.eot')
  src url('/fonts/artsy-icons.eot?#iefix') format('embedded-opentype'),
    url('/fonts/artsy-icons.ttf') format('truetype'),
    url('/fonts/artsy-icons.woff') format('woff'),
    url('/fonts/artsy-icons.svg#artsy-icons') format('svg')
  font-weight normal
  font-style normal

.order-form-cc-brands
  background-image: url('/images/cc_brands.png')

.order-form-geotrust
  background-image: url('/images/geotrust.png')

.bordered-select
  background-image: url('/images/select_arrow.png')

.mobile-section-iphone
  background-image: url('/images/fair-iphone-promo.png')
